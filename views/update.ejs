
<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form class="" action="/update" method="post">
      <div class="form-group">
        <label>Title</label>
        <input class="form-control" type="text" name="entryTitle" value="<%=title%>">
        <label>Entry</label>
        <textarea class="form-control" name="entryBody" rows="5" cols="30">
            <%=content%>
        </textarea>
      </div>
      <button class="btn btn-primary" type="submit" name="button">Update</button>
    </form>

<%- include("partials/footer"); -%>
